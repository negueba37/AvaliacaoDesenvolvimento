    function Delete : iModelDAOInterfaces<T>;
    function &End : iModelDAOInterfaces<T>;
    function Get : iModelDAOInterfaces<T>;
    function Insert : iModelDAOInterfaces<T>;
    function Update : iModelDAOInterfaces<T>;
    function This : T;
  end;
